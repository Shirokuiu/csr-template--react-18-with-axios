{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "ESLINT_NO_DEV_ERRORS='true' react-scripts start",
    "build": "react-scripts build",
    "generate:svg-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/_sprite.svg --stripAttrs id",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax=scss -q",
    "prettier": "npx prettier --write .",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "1.2.0",
    "formik": "2.2.9",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-helmet-async": "1.3.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.3",
    "react-scripts": "5.0.0",
    "react-simple-modal-provider": "1.9.0",
    "reselect": "4.1.5",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "3.7.1",
    "@jedmao/redux-mock-store": "3.0.5",
    "@reduxjs/toolkit": "1.9.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.1",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "14.1.0",
    "@types/jest": "27.4.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-helmet": "6.1.5",
    "@types/react-redux": "7.1.23",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "postcss": "8.4.12",
    "prettier": "2.8.0",
    "sass": "1.50.1",
    "stylelint": "14.15.0",
    "stylelint-declaration-use-variable": "1.7.3",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.3.0",
    "svg2sprite-cli": "2.0.1",
    "typescript": "4.9.3"
  },
  "lint-staged": {
    "*.{tsx,ts}": "eslint --fix",
    "*.scss": "stylelint --fix \"src/**/*.scss\" --syntax=scss -q"
  }
}
